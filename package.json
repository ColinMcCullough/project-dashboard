{
  "name": "project-dashboard",
  "version": "1.2.9",
  "description": "A full stack app template for the G5 OpEx team.",
  "author": "David Miller & Tyler Hasenoehrl & Colin McCullough",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ColinMcCullough/project-dashboard.git"
  },
  "config": {
    "imageRepo": "g5opex/project-dashboard",
    "imageName": "project-dashboard"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev:token": "cross-env TOKEN=$TOKEN NODE_ENV=development nodemon server/index.js --watch server",
    "docker:build": "cross-env docker build -t $npm_package_config_imageName . --build-arg NPM_TOKEN=$NPM_TOKEN",
    "docker:tag": "cross-env docker tag $npm_package_config_imageName $npm_package_config_imageRepo:$npm_package_version",
    "docker:push": "cross-env docker push $npm_package_config_imageRepo:$npm_package_version",
    "helm:upgrade": "helm upgrade project-dashboard ./chart/nuxt",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "deploy": "docker build -t $npm_package_config_imageName . --build-arg NPM_TOKEN=$NPM_TOKEN && docker tag $npm_package_config_imageName $npm_package_config_imageRepo:$npm_package_version && docker push $npm_package_config_imageRepo:$npm_package_version && helm upgrade crs ./chart/nuxt && kubectl get pods"
  },
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@google-cloud/pubsub": "^2.6.0",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/cloudinary": "^1.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "bull": "^3.18.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "i": "^0.3.6",
    "jquery": "^3.5.1",
    "npm": "^6.14.8",
    "nuxt": "^2.13.3",
    "path": "^0.12.7",
    "pg": "^7.18.2",
    "sequelize": "^5.22.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.9",
    "sass-loader": "^8.0.2",
    "vue-jest": "^3.0.6"
  }
}
